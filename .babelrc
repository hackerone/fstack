{
  "presets": ["es2015", "stage-1"],
  "plugins": [
    "syntax-flow",
    "transform-flow-strip-types",
    "static-fs",
    ["module-alias", [
      { "src": "./server/models", "expose": "models" },
      { "src": "./server/types", "expose": "types" },
      { "src": "./server/mutations", "expose": "mutations" },
      { "src": "./server/lib", "expose": "lib" },
      { "src": "./", "expose": "~" },
      { "src": "./server", "expose": "server" },
      { "src": "./client", "expose": "client" },
      { "src": "./client/containers", "expose": "containers" },
      { "src": "./client/components", "expose": "components" }
    ]]
  ]
}
